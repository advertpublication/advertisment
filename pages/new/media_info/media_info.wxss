page{
  padding: 20rpx 20rpx 0 20rpx;
  box-sizing: border-box;
  font-size: 26rpx;
}

.table {
  border: 0px solid darkgray;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}

.td {
  width: 40%;
  justify-content: center;
  text-align: center;
}

.bg-w {
  background: snow;
}

.bg-g {
  background: #e6f3f9;
}

.th {
  width: 40%;
  justify-content: center;
  background: #00b0f0;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
